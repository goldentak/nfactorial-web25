{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "types": ["vite/client"]
    },
    "include": ["src"]
}
